{
  "compilerOptions": {
    "mapRoot": "./sketch/",
    "module": "ESNext",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strict": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "rootDir": "./sketch/",
    "sourceMap": true,
    "sourceRoot": "./sketch/",
    "target": "es5",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
