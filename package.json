{
  "name": "p5ts-toy",
  "version": "1.0.0",
  "description": "Project to quickly get something working in [p5.js](https://p5js.org/) and [typescript](https://www.typescriptlang.org/)",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-old": "run-p start-compile start-run",
    "start-compile": "tsc --watch",
    "start-run": "browser-sync start --server -w",
    "start": "parcel serve index.html",
    "build": "cross-env NODE_ENV=production parcel build index.html --public-url /p5ts-toy/",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "homepage": "https://github.com/DrJKL/p5ts-toy#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-typescript": "^7.14.5",
    "browser-sync": "^2.26.14",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.2.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-beta.3.1",
    "parcel-bundler": "^1.12.5",
    "sass": "^1.35.1",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@types/p5": "^1.3.0",
    "babel-types": "^6.26.0",
    "p5": "^1.3.1"
  }
}
